

@{
    ViewData["Title"] = "Dashboard";

}

@if (TempData["SuccessMessage"] != null)
{
    <div class="alert alert-success alert-dismissable">
        <strong>@TempData["SuccessMessage"]</strong>
    </div>
}

<h2>Dashboard</h2>

<div class="container">

</div>

<ul>
    @foreach (var record in ViewData["entries"] as IList<UserUpload>)
    {
        <li>
            @record.Name
        </li>
        <li>
            @record.CreatedDate
        </li>
        <li>
            @record.fileName
        </li>
    }
</ul>

