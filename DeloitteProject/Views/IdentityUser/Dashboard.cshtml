

@{
    ViewData["Title"] = "Dashboard";

}

@if (TempData["SuccessMessage"] != null)
{
    <div class="alert alert-success alert-dismissable">
        <strong>@TempData["SuccessMessage"]</strong>
    </div>
}

<h2>Dashboard</h2>

<div class="container">
    <div class="col-2">
        
    </div>
    <div class="col-8">
        @foreach (var record in ViewData["entries"] as IList<UserUpload>)
        {
            <a asp-controller="IdentityUser" asp-action="ViewIndividual" asp-route-id="@record.Id">@record.fileName uploaded by @record.Name</a>
            <br />
        }
    </div>
    <div class="col-2">

    </div>
</div>

